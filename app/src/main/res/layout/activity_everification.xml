<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/everfy"
    tools:context=".everification">

    <TextView
        android:id="@+id/everify_Text"
        android:layout_width="338dp"
        android:layout_height="237dp"
        android:layout_marginStart="99dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="312dp"
        android:layout_marginBottom="620dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textSize="33sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.211"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.239" />

    <ImageView
        android:id="@+id/verifybutt"
        android:layout_width="137dp"
        android:layout_height="104dp"
        android:layout_marginTop="120dp"
        android:onClick="checked"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/everify_Text"
        app:srcCompat="@drawable/verify" />

</androidx.constraintlayout.widget.ConstraintLayout>